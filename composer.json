{
    "name": "dmx/support-toolset",
    "description": "A collection of support classes and traits.",
    "keywords": [
        "datamedrix",
        "support",
        "toolset",
        "tools",
        "collection"
    ],
    "license": "MIT",
    "type": "library",
    "version": "6.0.0",
    "require": {
        "php": "^8.2.0",
        "illuminate/contracts": "^8.0.0|^9.0.0",
        "illuminate/database": "^8.0.0|^9.0.0",
        "illuminate/support": "^8.0.0|^9.0.0",
        "illuminate/http": "^8.0.0|^9.0.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "DMX\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMX\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "style-fix": "@php vendor/bin/php-cs-fixer fix",
        "style-check": "@php vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    }
}
