{
    "name": "dmx/support-toolset",
    "description": "A collection of support classes and traits.",
    "keywords": [
        "datamedrix",
        "support",
        "toolset",
        "tools",
        "collection"
    ],
    "license": "MIT",
    "type": "library",
    "version": "4.3.0",
    "require": {
        "php": "^7.4.0|^8.0.0",
        "illuminate/contracts": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/database": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/support": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/http": "^6.0.0|^7.0.0|^8.0.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "DMX\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMX\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    }
}
