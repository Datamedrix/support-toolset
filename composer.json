{
    "name": "dmx/support-toolset",
    "description": "A collection of support classes and traits.",
    "keywords": ["datamedrix", "support", "toolset", "tools", "collection"],
    "license": "MIT",
    "type": "library",
    "version": "1.1.2",
    "require": {
        "php": "^7.2.0",
        "illuminate/support": "^5.6.0|^6.0.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.15",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "DMX\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMX\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    }
}
