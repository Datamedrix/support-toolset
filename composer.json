{
    "name": "dmx/support-toolset",
    "description": "Collection of support classes and traits.",
    "keywords": ["datamedrix", "support", "toolset", "tools", "collection"],
    "license": "MIT",
    "type": "library",
    "version": "0.0.0",
    "require": {
        "php": "^7.2.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "~1.7.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DMX\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMX\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    }
}
